	<queryString>
		<![CDATA[SELECT
	ped.NPedido, ped.FechaPedido, ped.Cliente, ped.Contacto, ped.ImporteBruto, ped.FormaPago, ped.PedidoCliente, ped.Comentarios,
	lin.Descripcion, lin.PrecioNeto, lin.Unidades, lin.TotalNeto, lin.Articulo, lin.Color, lin.Modelo, lin.Talla1, lin.Talla2, lin.Talla3, lin.Talla4, lin.Talla5, lin.Talla6, lin.Talla7, lin.Talla8, lin.Talla9, lin.Talla10, lin.Talla11, lin.Talla12, lin.Talla13, lin.Talla14, lin.Talla15, lin.Talla16, lin.Talla17, lin.Pedidas1, lin.Pedidas2, lin.Pedidas3, lin.Pedidas4, lin.Pedidas5, lin.Pedidas6, lin.Pedidas7, lin.Pedidas8, lin.Pedidas9, lin.Pedidas10, lin.Pedidas11, lin.Pedidas12, lin.Pedidas13, lin.Pedidas14, lin.Pedidas15, lin.Pedidas16, lin.Pedidas17,
