	<queryString>
		<![CDATA[SELECT alb.NAlbaran, alb.PedidoCliente, alb.Cliente, alb.FechaEnvio, alb.Contacto, alb.Unidades UnidadesTotales, alb.Comentarios,
	cli.CIF, cli.Direccion, cli.Poblacion, cli.Provincia, cli.Postal, cli.Telefono, cli.Fax,
